<% @places.each do |place| %>
  <div class="booyah-box" col-10 offest-1>
    <h1><%= place.name %></h1>
    <i><%= place.address %></i><br /><br />
    <p><%= place.description %></p><br />
    <!--<p><%= place.id %></p><br />-->

    <% if place.user.present? %>
      <small><%= place.user.email %></small>
    <% end %>
  </div>

<% end %>

<%= will_paginate @place, renderer: BootstrapPagination::Rails, previous_label: "Older", next_label: "Newer", iner_window: 1, outer_window: 0 %>